D <- readDiscreteCharacterData(file="morpho.nex")
n_taxa <- D.ntaxa()
n_sites <- D.nchar(1)
names <- D.names()
n_branches <- 2 * n_taxa - 3
mi = 1
Q := fnJC(3)
T <- readTrees("morpho.tre")[1]
phylogeny <- T
scaleRate <- 1.0
seq ~ dnPhyloCTMCFiltered(tree=phylogeny, Q=Q, branchRates=scaleRate, nSites=n_sites, type="Standard")
seq.clamp(D)
structure(seq)
q()

